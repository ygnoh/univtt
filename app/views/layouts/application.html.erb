	<%= csrf_meta_tags %>
</head>
<body>
	<!-- Modal -->
	<%= render "layouts/modal" %>

	<!-- Navigator -->
	<%= render "layouts/header" %>

	<!-- Flash -->
	<div id="flash" style="width: 100%; position: fixed; top: 0; z-index: 10;">
		<%= render "layouts/flash" %>
	</div>

	<%= yield %>
	
</body>
</html>
