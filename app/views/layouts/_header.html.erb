<ul>
	<li><%= link_to "home", "/" %></li>
	<li><%= link_to "시간표 생성하기", "/timetable/new" %></li>
	<li><%= link_to "시간표 추천", "/recommend/index" %></li>
	<li><%= link_to "빈강의실 찾기", "/classroom/index" %></li>
	<% if !user_signed_in? %>
		<li><%= link_to "로그인", "/users/sign_in" %></li>
	<% else %>
		<li><%= link_to "나의시간표", "/timetable/show/#{current_user.id}" %></li>
			<% if current_user.admin? %>
				<li><%= link_to "ADMIN", "/admin" %></li>
			<% end %>
		<li><%= link_to "로그아웃", "/users/sign_out", method: :delete %></li>
		<li><%= current_user.username %></li>
	<% end %>
</ul>
